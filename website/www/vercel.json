{
  "rewrites": [
    {
      "source": "/docs/",
      "destination": "https://niledatabase-docs.vercel.app/"
    },
    {
      "source": "/docs/:match*",
      "destination": "https://niledatabase-docs.vercel.app/:match*"
    },
    {
      "source": "/guides/",
      "destination": "https://niledatabase-docs.vercel.app/guides/"
    },
    {
      "source": "/guides/:match*",
      "destination": "https://niledatabase-docs.vercel.app/guides/:match*"
    }
  ]
}
